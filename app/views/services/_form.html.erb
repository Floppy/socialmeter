<%= form_for [@user, @service] do |f| %>

  <%= render "shared/error_messages", :target => @service %>

  <ul>
    <li>
      Service Name: <%= f.select :name, Service.service_list %>
    </li>
    <li>
      External ID: <%= f.text_field :external_id %>
    </li>
  </ul>

  <%= f.submit %>

<% end %>
